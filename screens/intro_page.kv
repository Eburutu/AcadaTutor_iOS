<IntroPage>:
    BoxLayout:
        orientation: 'vertical'
        size_hint_y: 1

        canvas.before:
            Color:
                rgba: 0.4, 0.508, 0.33, 0 # Set the background color for the FloatLayout (R, G, B, A)
            Rectangle:
                pos: self.pos
                size: self.size

        # AdMob Banner Ad
        # BoxLayout:
        #     orientation: 'vertical'
        #     size_hint_y: 0.25


        AdmobBanner:
            size_hint_y: None
            id: banner_layout
            height: 70



        FloatLayout:
            orientation: "horizontal"
            canvas.before:
                Color:
                    rgba: 0.4, 0.508, 0.33, 0  # Set the background color for the FloatLayout (R, G, B, A)
                Rectangle:
                    pos: self.pos
                    size: self.size

            size_hint_y: 0.95  # Fix the height of the FloatLayout
            height: dp(100)  # Set the absolute pixel height of the FloatLayout
            pos_hint: {'center_x': 0.5, 'y': 0} 


            ImageButton3:
                source: 'first_t/First_English.png'
                size_hint: None, None
                # on_release: root.goto_Edubot()
                # size: dp(260), dp(200)  # Adjust the size as needed
                size: self.parent.width * 0.50, self.parent.height * 0.85
                pos_hint: {'x': 0.05, 'center_y': 0.5}
